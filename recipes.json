  "walnut-cranberry-sourdough": {
    "id": "walnut-cranberry-sourdough",
    "name": "Walnut Cranberry Sourdough - Single Loaf",
    "tags": ["Bread", "Sourdough", "Artisan"],
    "totalTime": "25 hours",
    "yield": "1 loaf",
    "dateAdded": 1703123400000,
    "ingredients": [
      {
        "groupName": "Levain",
        "items": [
          {"amount": 20, "unit": "g", "item": "bread flour or all-purpose flour"},
          {"amount": 20, "unit": "g", "item": "whole rye flour"},
          {"amount": 20, "unit": "g", "item": "sourdough starter"},
          {"amount": 40, "unit": "g", "item": "water"}
        ]
      },
      {
        "groupName": "Dough",
        "items": [
          {"amount": 400, "unit": "g", "item": "bread flour"},
          {"amount": 300, "unit": "g", "item": "water"},
          {"amount": 100, "unit": "g", "item": "levain (from above)"},
          {"amount": 8, "unit": "g", "item": "salt"},
          {"amount": 75, "unit": "g", "item": "toasted walnuts, chopped"},
          {"amount": 60, "unit": "g", "item": "dried cranberries"}
        ]
      }
    ],
    "instructions": [
      {
        "timeHeader": "Day 1",
        "steps": [
          {"time": "9:00 PM - Make Levain", "text": "Mix levain ingredients in a small bowl. Cover and let sit at room temperature overnight (8-12 hours)."}
        ]
      },
      {
        "timeHeader": "Day 2",
        "steps": [
          {"time": "9:00 AM - Autolyse", "text": "Mix flour and water until just combined. Cover and let rest for 30 minutes."},
          {"time": "9:30 AM - Mix Dough", "text": "Add levain and salt to the flour mixture. Mix by hand until well combined. Perform 4 sets of coil folds 30 minutes apart."},
          {"time": "11:30 AM - Add Mix-ins", "text": "Gently fold in toasted walnuts and cranberries during the last set of folds."},
          {"time": "12:00 PM - Bulk Ferment", "text": "Let dough rise in a covered container for 4-5 hours at room temperature until increased by 50%."},
          {"time": "5:00 PM - Pre-shape", "text": "Turn dough onto lightly floured surface and pre-shape into a round. Rest 20 minutes."},
          {"time": "5:20 PM - Final Shape", "text": "Shape into a boule or batard. Place seam-side up in a banneton or bowl lined with floured cloth."},
          {"time": "5:30 PM - Cold Proof", "text": "Cover and refrigerate overnight (12-48 hours)."}
        ]
      },
      {
        "timeHeader": "Day 3 - Baking Day",
        "steps": [
          {"time": "9:00 AM - Preheat", "text": "Place Dutch oven in oven and preheat to 475°F (245°C) for 45 minutes."},
          {"time": "9:45 AM - Score", "text": "Turn dough onto parchment paper. Score with a sharp knife or razor blade."},
          {"time": "10:00 AM - Bake", "text": "Carefully transfer dough (on parchment) to hot Dutch oven. Cover and bake 20 minutes."},
          {"time": "10:20 AM - Uncover", "text": "Remove lid and continue baking 15-20 minutes until golden brown and internal temp reaches 205°F (96°C)."},
          {"time": "10:40 AM - Cool", "text": "Remove from oven and cool on wire rack for at least 1 hour before slicing."}
        ]
      }
    ],
    "notes": [
      "All measurements are halved from original recipe for a single loaf",
      "Toast walnuts at 350°F for 8-10 minutes before chopping",
      "Bread will keep wrapped at room temperature for 3 days or frozen for up to 3 months",
      "For best results, use a kitchen scale to measure ingredients by weight"
    ]
  },
  "classic-chocolate-chip-cookies": {
    "id": "classic-chocolate-chip-cookies",
    "name": "Classic Chocolate Chip Cookies",
    "tags": ["Desserts", "Quick & Easy", "American", "Cookies"],
    "totalTime": "30 minutes",
    "yield": "24 cookies",
    "dateAdded": 1703209800000,
    "ingredients": [
      {
        "groupName": null,
        "items": [
          {"amount": 2.25, "unit": "cups", "item": "all-purpose flour"},
          {"amount": 1, "unit": "tsp", "item": "baking soda"},
          {"amount": 1, "unit": "tsp", "item": "salt"},
          {"amount": 1, "unit": "cup", "item": "butter, softened"},
          {"amount": 0.75, "unit": "cup", "item": "granulated sugar"},
          {"amount": 0.75, "unit": "cup", "item": "brown sugar, packed"},
          {"amount": 2, "unit": "large", "item": "eggs"},
          {"amount": 2, "unit": "tsp", "item": "vanilla extract"},
          {"amount": 2, "unit": "cups", "item": "chocolate chips"}
        ]
      }
    ],
    "instructions": [
      {
        "timeHeader": null,
        "steps": [
          {"time": null, "text": "Preheat oven to 375°F (190°C). Line baking sheets with parchment paper."},
          {"time": null, "text": "In a medium bowl, whisk together flour, baking soda, and salt. Set aside."},
          {"time": null, "text": "In a large bowl, cream together softened butter and both sugars until light and fluffy, about 3-4 minutes."},
          {"time": null, "text": "Beat in eggs one at a time, then add vanilla extract."},
          {"time": null, "text": "Gradually mix in the flour mixture until just combined. Don't overmix."},
          {"time": null, "text": "Fold in chocolate chips with a wooden spoon or spatula."},
          {"time": null, "text": "Drop rounded tablespoons of dough onto prepared baking sheets, spacing 2 inches apart."},
          {"time": null, "text": "Bake for 9-11 minutes until edges are golden but centers still look slightly underbaked."},
          {"time": null, "text": "Cool on baking sheet for 5 minutes, then transfer to wire rack to cool completely."}
        ]
      }
    ],
    "notes": [
      "For chewier cookies, slightly underbake them",
      "For crispier cookies, bake an extra 1-2 minutes",
      "Store in airtight container for up to 1 week",
      "Dough can be refrigerated for up to 3 days or frozen for up to 3 months"
    ]
  },
